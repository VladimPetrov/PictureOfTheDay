<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Primary" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="headerTextColor">@color/black</item>
        <item name="bottomSheetBG">@color/title_background_theme_1</item>
        <item name="chipBGColor">@color/chip_background_color_theme_1</item>
        <item name="RBTintColor">@color/radiobutton_tint_color_theme_1</item>
        <item name="textILStrokeColor">@color/text_input_stroke_color_theme_1</item>
        <item name="textILTintColor">@color/text_input_tint_color_theme_1</item>
        <item name="fabBackgroundTint">@color/fab_color_theme_1</item>
        <item name="fabIconColor">@color/fab_tint_color_theme_1</item>
        <item name="bottomNavBackgroundColor">@color/bottom_nav_background_color_theme_1</item>
        <item name="bottomNavIconColor">@color/bottom_nav_icon_color_theme_1</item>
        <item name="dotIndicateColor">@color/dot_indicate_color_theme_1</item>
        <item name="titleBackground">@color/title_background_theme_1</item>
        <!-- Customize your theme here. -->
    </style>
